package com.liming.utils;

public class Keyword {

    public String getWord() {
        return word;
    }

    public void setWord(String word) {
        this.word = word;
    }

    public String getPos() {
        return pos;
    }

    public void setPos(String pos) {
        this.pos = pos;
    }

    public boolean isStop() {
        return stop;
    }

    public void setStop(boolean stop) {
        this.stop = stop;
    }

    public boolean isImportant() {
        return important;
    }

    public void setImportant(boolean important) {
        this.important = important;
    }

    public int getFrequency() {
        return frequency;
    }

    public void setFrequency(int frequency) {
        this.frequency = frequency;
    }

    public double getTf() {
        return tf;
    }

    public void setTf(double tf) {
        this.tf = tf;
    }

    public double getIdf() {
        return idf;
    }

    public void setIdf(double idf) {
        this.idf = idf;
    }

    public double getTfidf() {
        return tfidf;
    }

    public void setTfidf() {
        this.tfidf = this.tf * this.idf;
    }

    public void setTfidf(double tfidf) {
        this.tfidf = tfidf;
    }

    public double getTextrank() {
        return textrank;
    }

    public void setTextrank(double textrank) {
        this.textrank = textrank;
    }

    private String word = null;

    private String pos = null;

    private boolean stop = true;

    private boolean important = false;

    private int frequency = 1;

    private double tf = 0.0;

    private double idf = 0.0;

    private double tfidf = 0.0;

    private double textrank = 0.0;

}
