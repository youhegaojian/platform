  <select id="getPatentList" resultType="com.liming.domain.ApscAnalysisPatent">
  	SELECT 
  		id,
  		title,
  		abstract_ abstract,
  		desc_keywords descKeywords,
  		publication_day as publicationDay,
  		title_keywords as titleKeywords,
  		abstract_keywords as abstractKeywords
  	from apsc_analysis_patent 
  	WHERE 
  		task_id=#{taskId} 
  
  </select>
  
  <insert id="insertPatentList" parameterType="com.liming.domain.ApscAnalysisPatent">
  	insert into apsc_analysis_patent(
  		id,
  		publication_day,
  		title_keywords,
  		abstract_keywords
  	)values
  	<foreach collection="list" item="patent" separator=",">
  		(
  			#{patent.id},
  			#{patent.publicationDay},
  			#{patent.titleKeywords},
  			#{patent.abstractKeywords}
  		)
  	</foreach>
  	on duplicate key update 
  	title_keywords = values(title_keywords),
  	abstract_keywords = values(abstract_keywords)
  	
  </insert>
  
  
